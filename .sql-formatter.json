{
	"$schema": "https://raw.githubusercontent.com/sql-formatter-org/sql-formatter/refs/heads/master/schema.json",
	"language": "postgresql",
	"tabWidth": 2,
	"keywordCase": "upper",
	"linesBetweenQueries": 1,
	"dataTypeCase": "upper",
	"functionCase": "upper",
	"identifierCase": "lower",
	"newlineBeforeSemicolon": false,
	"logicalOperatorNewline": "before"
}
